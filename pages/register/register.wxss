.btn-area{
  padding: 20rpx;
}
.warning{
    background-color: rgb(255, 240 ,240);
    width: 90%;
    border-radius: 20rpx;
    box-shadow: 5rpx 5rpx 20rpx 2rpx rgb(210, 210, 210);
    margin-bottom: 20rpx;
}
.icon{
    padding-top: 30rpx;
    padding-left: 50rpx;
    height: 70rpx;
    width: 70rpx;
}
.labor{
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    padding-left: 50rpx;
    font-size: 32rpx;
