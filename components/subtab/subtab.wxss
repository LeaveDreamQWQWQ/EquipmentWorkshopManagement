.subitem {
    height: auto;
    width: 100%;
    display: flex;
    justify-content: space-around;
    align-content: space-around;
    background-color: #E3E7E8;
}

.log {
    height: auto;
    /* width: auto; */
    display: flex;
    justify-content: center;
    align-content: space-around;
    flex-wrap: wrap;
    /* border-radius: 5%; */

}

.subsubitem {
    width: 600rpx;
    height: 150rpx;
    display: flex;
    align-items: center;
    border-radius: 1ch;
    background-color: rgb(226, 225, 225);
    margin-top: 34rpx;
    box-shadow:3px 3px 5px 0 rgb(189, 189, 189), -3px -3px 6px 2px rgb(255, 255, 255);
    font-weight: 500;
    font-size: 40rpx;
}

.isAvailable {
    border: 4px solid #41BF78;
}

.disAvailable {
    border: 4px solid rgb(223, 176, 106);
}

.False {
    border: 4px solid rgb(243, 114, 114);
}

.Using{
  border: 4px solid rgb(114, 196, 243);
}


.item_icon {
    width: 120rpx;
    height: 120rpx;
    margin-left: 10px;
    border-radius: 50%;
    /* border: 4px solid rgb(127, 196, 136); */
    display: flex;
    justify-content: center;
    align-items: center;
}

image{
    width: 120%;
}

.name{
    margin-left: 25rpx;
    width: 200rpx;
    text-align: center;
}

.line{
    width: 3rpx;
    height: 100rpx;
    background-color: rgb(202, 201, 201);
    margin-left: 30rpx;
}

.state{
    width: 180rpx;
    text-align: center;
}

