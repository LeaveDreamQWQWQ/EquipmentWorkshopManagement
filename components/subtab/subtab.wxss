.Button {
    display: flex;
    justify-content: center;
    align-content: space-around;
    height: auto;
    flex-wrap: wrap;
}

.submission{
    font-weight: 700;
    font-size: 36rpx;
    /* background-color: rgb(243, 242, 242); */
    background-color: rgb(226, 225, 225);
    width: 100rpx;
    height: 100rpx;
    text-align: center;
    border-radius: 1ch;
    /* border:2px solid rgb(105, 103, 103); */
    margin-top: 30rpx;
    margin-right: 20rpx;
    /* box-shadow: 2px 2px 6px 1px rgb(190, 190, 190),0px 0px 5px 1px rgb(255, 255, 255); */
    box-shadow:3px 3px 5px 0 rgb(189, 189, 189), -1px -1px 6px 2px rgb(255, 255, 255);
}

.Fault_submission {
    color: #F27405;
}

.ReFault_submission {
    color: #0DA64F;
}

